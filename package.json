{
  "name": "whatsapp-api-baileys",
  "version": "1.0.0",
  "description": "WhatsApp API with Multi-Device Support - RESTful Approach",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --transpile-only --respawn --ignore-watch node_modules -r tsconfig-paths/register src/main/server/server.ts",
    "build": "tsc",
    "start": "node dist/main/server/server.js",
    "test:e2e": "jest --runInBand --detectOpenHandles --forceExit --testMatch **/*.e2e-spec.ts",
    "rabbitmq:consumers:dev": "npm run start:dev -- --entryFile cmd/rabbitmq",
    "rabbitmq:consumers:prod": "node dist/cmd/rabbitmq",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "repository": "git@github.com:yury-tomaz/whatsapp-api-baileys.git",
  "author": "Yury Tomaz <yurytomaz@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@faker-js/faker": "^8.4.0",
    "@types/amqplib": "^0.10.5",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mongodb": "^4.0.7",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.11",
    "@types/qrcode": "^1.5.5",
    "@types/semaphore": "^1.1.4",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.9.0",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@whiskeysockets/baileys": "^6.6.0",
    "amqplib": "^0.10.4",
    "axios": "^1.7.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
