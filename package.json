{
  "name": "whatsapp-api-baileys",
  "version": "1.0.0",
  "description": "WhatsApp API with Multi-Device Support - RESTful Approach",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --transpile-only --respawn --ignore-watch node_modules -r tsconfig-paths/register src/main/server/server.ts",
    "build": "tsc",
    "start": "node dist/main/server/server.js",
    "changelog:last": "conventional-changelog -p angular -r 2",
    "changelog:preview": "conventional-changelog -p angular -u",
    "changelog:update": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "jest --runInBand --detectOpenHandles --forceExit",
    "test:e2e": "jest --runInBand --detectOpenHandles --forceExit --testMatch **/*.e2e-spec.ts",
    "test:watch": "jest --watch  --runInBand --detectOpenHandles --forceExit --testMatch **/*.spec.ts"
  },
  "repository": "git@github.com:yury-tomaz/whatsapp-api-baileys.git",
  "author": "Yury Tomaz <yurytomaz@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@faker-js/faker": "^8.4.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/mongodb": "^4.0.7",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.8",
    "@types/qrcode": "^1.5.5",
    "@types/semaphore": "^1.1.4",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "conventional-changelog-cli": "^4.1.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@whiskeysockets/baileys": "^6.6.0",
    "axios": "^1.6.7",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "mongodb": "^6.3.0",
    "multer": "^1.4.5-lts.1",
    "pino": "^8.17.2",
    "pino-http": "^9.0.0",
    "pino-pretty": "^10.3.1",
    "qrcode": "^1.5.3",
    "qrcode-terminal": "^0.12.0",
    "semaphore": "^1.1.0",
    "uuid": "^9.0.1",
    "yup": "^1.3.3"
  }
}
