    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.6.2",
    "multer": "^1.4.5-lts.1",
    "pino": "^9.1.0",
    "pino-http": "^10.1.0",
    "pino-pretty": "^10.3.1",
    "qrcode": "^1.5.3",
    "qrcode-terminal": "^0.12.0",
    "semaphore": "^1.1.0",
    "uuid": "^9.0.1",
    "yup": "^1.4.0"
  }
}
